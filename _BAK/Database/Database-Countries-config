CREATE TABLE `Countries` (
`CountryID` int(11) NOT NULL auto_increment,
`CountryCode` varchar(2) NOT NULL default '',
`CountryName` varchar(100) NOT NULL default '',
PRIMARY KEY (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;
