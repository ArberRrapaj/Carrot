CREATE TABLE `Games` (
  `GameID` int(11) NOT NULL AUTO_INCREMENT,
  `GenreID` int(11) NOT NULL,
  `Title` varchar(100) CHARACTER SET utf8 COLLATE utf8_unicode_ci NOT NULL,
  `Publisher` varchar(100) CHARACTER SET utf8 COLLATE utf8_unicode_ci DEFAULT NULL,
  `Released` year(4) DEFAULT NULL,
  `LevelTypeID` int(11) NOT NULL,
  `Image` mediumblob,
  PRIMARY KEY (`GameID`),
  KEY `LevelTypeID` (`LevelTypeID`),
  KEY `GenreID` (`GenreID`),
  CONSTRAINT `games_ibfk_1` FOREIGN KEY (`GenreID`) REFERENCES `genres` (`genreid`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB AUTO_INCREMENT=65 DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci