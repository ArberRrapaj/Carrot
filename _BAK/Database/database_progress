CREATE TABLE `Progress` (
  `ProgressID` int(11) NOT NULL AUTO_INCREMENT,
  `UserID` int(11) NOT NULL,
  `GameID` int(11) NOT NULL,
  `LevelID` int(11) NOT NULL,
  `Completed` tinyint(1) NOT NULL,
  PRIMARY KEY (`ProgressID`),
  KEY `GameID` (`GameID`),
  KEY `LevelID` (`LevelID`),
  KEY `UserID` (`UserID`),
  CONSTRAINT `progress_ibfk_1` FOREIGN KEY (`GameID`) REFERENCES `games` (`gameid`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `progress_ibfk_2` FOREIGN KEY (`LevelID`) REFERENCES `levels` (`levelid`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `progress_ibfk_3` FOREIGN KEY (`UserID`) REFERENCES `users` (`userid`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci